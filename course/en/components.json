    "_graphic": {
      "src": "course/en/assets/696b9478f4c766f74fb6030bceca8d2fadf19bdf.JPG",
      "alt": "",
      "attribution": "source: National Audit Office. Recent lessons show weaknesses across a number of modelling programmes. The solutions are relatively simple which you can see in section two (demonstrate)"
    },
    "mobileBody": "",
    "mobileInstruction": "",
    "_hidePagination": false,
    "_canCycleThroughPagination": false,
    "_isNarrativeOnMobile": true,
    "_useNumberedPins": true,
